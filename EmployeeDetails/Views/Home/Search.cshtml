@model List<Employee>
<h3>Search Results</h3>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Search</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Salary</td>
            </tr>
        }
    </tbody>
</table>